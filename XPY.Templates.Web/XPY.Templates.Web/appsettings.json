{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Swagger": {
    "Name": "$swaggerName$",
    "Description": "$swaggerDescription$"
  },
  "JWT": {
    "Issuer": "$issuer$",
    "Audience": "$audience$",
    "SecureKey": "$securekey$",
    "Expires": "$expires$"
  },
  "ConnectionStrings": {
    "default": "$connectionString$"
  }
}
